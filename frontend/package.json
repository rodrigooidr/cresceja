{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@hello-pangea/dnd": "^16.6.0",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "msw": "1.3.2",
    "postcss": "^8.5.6",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "guardrail:inbox": "node scripts/guardrail-inbox.js"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(msw|@mswjs|axios|nanoid|uuid|@bundled-es-modules|tough-cookie|whatwg-url|formdata-polyfill|fetch-blob|undici)/)"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass|less)$": "identity-obj-proxy",
      "^tough-cookie$": "<rootDir>/src/test-shims/tough-cookie.js",
      "^@bundled-es-modules/tough-cookie(.*)$": "<rootDir>/src/test-shims/tough-cookie.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}